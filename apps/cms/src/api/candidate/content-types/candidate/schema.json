{
  "kind": "collectionType",
  "collectionName": "candidates",
  "info": {
    "singularName": "candidate",
    "pluralName": "candidates",
    "displayName": "Candidate",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "firstName": {
      "type": "string",
      "required": false
    },
    "lastName": {
      "type": "string",
      "required": false
    },
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "phone": {
      "type": "string",
      "regex": "^[+]*[(]{0,1}[0-9]{1,4}[)]{0,1}[-\\s\\./0-9]*$",
      "required": false
    },
    "position": {
      "type": "string",
      "required": false
    },
    "level": {
      "type": "string",
      "required": false
    },
    "resume": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "tests": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::test.test",
      "inversedBy": "candidates"
    },
    "comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment.comment",
      "mappedBy": "candidate"
    },
    "individualTests": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::individual-test.individual-test",
      "mappedBy": "candidate"
    }
  }
}
